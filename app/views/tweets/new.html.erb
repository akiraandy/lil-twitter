<div class="container text-center">
  <div class="row">
    <div class="col">
        <h1>Create new tweet</h1>
    </div>

  </div>
  <div class="row">
    <div class="col">
      <div>
        <%= form_for @tweet, class: "col" do |f| %>
          <%= render "shared/errors", object: @tweet %>
          <%= f.text_area :body %>
          <%= f.submit class: "btn btn-primary"%>
        <% end %>
      </div>
    </div>

  </div>
</div>


</div>
